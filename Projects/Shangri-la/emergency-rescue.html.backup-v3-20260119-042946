    <script>
        // å¹³æ»‘æ»šåŠ¨å’Œå¯¼èˆªé«˜äº®
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({ behavior: 'smooth', block: 'start' });
                }
            });
        });

        // æ»šåŠ¨æ—¶å¯¼èˆªæ é«˜äº®
        window.addEventListener('scroll', () => {
            const sections = document.querySelectorAll('.section-block');
            const navLinks = document.querySelectorAll('header .nav-links a');
            
            let current = '';
            sections.forEach(section => {
                const sectionTop = section.offsetTop - 150;
                if (pageYOffset >= sectionTop) {
                    current = section.getAttribute('id');
                }
            });

            navLinks.forEach(link => {
                link.classList.remove('active');
                if (link.getAttribute('href') === '#' + current) {
                    link.classList.add('active');
                }
            });
        });

        // åˆå§‹åŒ–Pannellumå…¨æ™¯æŸ¥çœ‹å™¨
        let viewer;
        let isAutoRotating = false;

        window.addEventListener('load', () => {
            console.log('é¡µé¢åŠ è½½å®Œæˆï¼Œå¼€å§‹åˆå§‹åŒ–å…¨æ™¯æŸ¥çœ‹å™¨');
            try {
                viewer = pannellum.viewer('panorama-viewer', {
                    "type": "equirectangular",
                    "panorama": "images/å“ˆå·´é›ªä¸Šè¥åœ°é€‰å€360å…¨æ™¯å›¾.jpeg",
                    "autoLoad": true,
                    "autoRotate": 0,
                    "autoRotateInactivityDelay": 3000,
                    "showControls": true,
                    "showFullscreenCtrl": false,
                    "showZoomCtrl": true,
                    "mouseZoom": true,
                    "draggable": true,
                    "friction": 0.15,
                    "hfov": 100,
                    "minHfov": 50,
                    "maxHfov": 120,
                    "pitch": 0,
                    "yaw": 0,
                    "compass": true,
                    "northOffset": 0,
                    "strings": {
                        "loadButtonLabel": "ç‚¹å‡»åŠ è½½å…¨æ™¯",
                        "loadingLabel": "åŠ è½½ä¸­...",
                        "bylineLabel": "å“ˆå·´é›ªå±±è¥åœ°é€‰å€360Â°å…¨æ™¯"
                    }
                });
                console.log('å…¨æ™¯æŸ¥çœ‹å™¨åˆå§‹åŒ–æˆåŠŸ:', viewer);
            } catch (error) {
                console.error('å…¨æ™¯æŸ¥çœ‹å™¨åˆå§‹åŒ–å¤±è´¥:', error);
            }
        });

        // é‡ç½®è§†è§’
        function resetView() {
            if (viewer) {
                viewer.setPitch(0);
                viewer.setYaw(0);
                viewer.setHfov(100);
            }
        }

        // åˆ‡æ¢è‡ªåŠ¨æ—‹è½¬
        function toggleAutoRotate() {
            console.log('toggleAutoRotateè¢«è°ƒç”¨');
            console.log('viewerå¯¹è±¡:', viewer);
            console.log('isAutoRotatingçŠ¶æ€:', isAutoRotating);
            
            const btn = document.getElementById('autoRotateBtn');
            if (viewer) {
                if (isAutoRotating) {
                    viewer.stopAutoRotate();
                    btn.innerHTML = '<i class="fas fa-play"></i> è‡ªåŠ¨æ—‹è½¬';
                    isAutoRotating = false;
                    console.log('å·²åœæ­¢è‡ªåŠ¨æ—‹è½¬');
                } else {
                    viewer.startAutoRotate(-4);
                    btn.innerHTML = '<i class="fas fa-pause"></i> åœæ­¢æ—‹è½¬';
                    isAutoRotating = true;
                    console.log('å·²å¼€å§‹è‡ªåŠ¨æ—‹è½¬');
                }
            } else {
                console.error('viewerå¯¹è±¡æœªåˆå§‹åŒ–');
                alert('å…¨æ™¯æŸ¥çœ‹å™¨å°šæœªåŠ è½½å®Œæˆï¼Œè¯·ç¨åå†è¯•');
            }
        }

        // å…¨å±æŸ¥çœ‹
        function toggleFullscreen() {
            if (viewer) {
                viewer.toggleFullscreen();
            }
        }

        // å›¾ç‰‡æ¨¡æ€æ¡†
        function openModal(imageSrc) {
            const modal = document.getElementById('imageModal');
            const modalImg = document.getElementById('modalImage');
            modal.style.display = 'block';
            modalImg.src = imageSrc;
            document.body.style.overflow = 'hidden';
        }

        function closeModal() {
            const modal = document.getElementById('imageModal');
            modal.style.display = 'none';
            document.body.style.overflow = 'auto';
        }

        // ç‚¹å‡»æ¨¡æ€æ¡†èƒŒæ™¯å…³é—­
        document.getElementById('imageModal').addEventListener('click', function(e) {
            if (e.target === this) {
                closeModal();
            }
        });

        // ESCé”®å…³é—­æ¨¡æ€æ¡†
        document.addEventListener('keydown', function(e) {
            if (e.key === 'Escape') {
                closeModal();
            }
        });

        // é¡µé¢åŠ è½½åŠ¨ç”»
        window.addEventListener('load', () => {
            document.querySelectorAll('.section-block').forEach((section, index) => {
                section.style.animationDelay = `${index * 0.1}s`;
            });
        });
    </script>
